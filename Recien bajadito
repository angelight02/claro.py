def print_lyrics():
    print('Hay sola')
    print('No hay que decirle nah serena tranquila si quieren')  
print_lyrics()

def repeat_lyrics():
    print_lyrics()
    print_lyrics()

def print_twice(x):
    print(x)
    print(x)

print_twice(repeat_lyrics())

def statements_local(part1, part2):
    mix = part1 + part2
    print_twice(mix)
    
